{
   "_id": "_design/clients",
   "language": "javascript",
   "views": {
       "active": {
         "map": "function(doc) {\n  if(!doc.cancelled) {\n    emit(doc._id, doc);\n  }\n}"
       },
			 "segmentStatus": {
				 "map": "function(doc) {\n  if(!doc.cancelled) {\n    emit(doc.segmentName);\n  }\n}",
				 "reduce": "_count"
			 }
   }
}